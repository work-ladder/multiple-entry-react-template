{
  "name": "hellowebpack",
  "version": "1.0.0",
  "description": "webpack学习项目",
  "main": "index.js",
  "scripts": {
    "test": "node test.js --test",
    "build": "node script/build.js  && webpack --config script/webpack.config.prod.js --env=production --progress",
    "start": "node script/start.js && webpack-dev-server --config script/webpack.config.dev.js  --env=development",
    "serve": "node script/server.js",
    "lint": "eslint --fix --ext .js --ext .jsx src"
  },
  "author": "hwx",
  "license": "ISC",
  "browserslist": [
    "defaults",
    "not ie <= 8",
    "last 2 versions",
    "> 1%",
    "iOS >= 7",
    "Android >= 4.0"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "inquirer": "^7.3.0",
    "less": "^3.11.3",
    "less-loader": "^6.1.3",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react-dev-utils": "^10.2.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@gem-mine/durex": "^2.1.0",
    "@gem-mine/durex-router": "^1.4.3",
    "@gem-mine/react-loadable": "^5.5.0",
    "@sdp.nd/fish": "^3.11.1",
    "axios": "^0.19.2",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "mocker-api": "^2.3.5",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.0"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ],
      [
        "import",
        {
          "libraryName": "fish",
          "libraryDirectory": "lib",
          "style": true
        }
      ]
    ]
  },
  "jsconfig": {
    "compilerOptions": {
      "experimentalDecorators": true
    }
  }
}
